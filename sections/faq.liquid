{% for block in section.blocks %}
  {% case block.type %}
    {% when 'QA' %}
      <h3>{{block.settings.question}}</h3>
      <h5>{{block.settings.answer}}</h5>
    {% when 'HEADER' %}
     <h1>{{block.settings.heading}}</h1>
  {% endcase %}
{% endfor %}

{% schema %}
  {
    "name": "Section name",
      "blocks":[
         {
           "name": "QA",
           "type": "QA",
           "settings": [
              {
                "type": "text",
                "id": "question",
                "label": "Question"
              },
             {
              "type": "textarea",
              "id": "answer",
              "label": "Answer"
            }
           ]
         },
         {
           "name": "HEADER",
           "type": "HEADER",
           "settings": [
              {
               "type": "text",
                "id": "heading",
                "label": "Heading"
              }
           ]
          }
        
  ],
  "settings": [],
     "presets": [
    {
      "name": "faq"
    }
  ]
  }
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

